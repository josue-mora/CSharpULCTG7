<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LCSharpMBG7.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LCSharpMBG7"
    Shell.FlyoutBehavior="Disabled"
    Title="LCSharpMBG7">

    <ShellContent
        Title="TestRoom"
        ContentTemplate="{DataTemplate local:Views.TestRoom}"
        Route="Views.TestRoom" />

    <ShellContent
        Title="VehicleDetails"
        ContentTemplate="{DataTemplate local:Views.VehicleDetails}"
        Route="Views.VehicleDetails" />

    <ShellContent
        Title="FirebaseDataPage"
        ContentTemplate="{DataTemplate local:Views.FirebaseDataPage}"
        Route="Views.FirebaseDataPage" />

</Shell>
